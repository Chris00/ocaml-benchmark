#!/bin/sh

BZR=bzr.dev
USER=chris_77
PROJECT=ocaml-benchmark
SF=sourceforge.net

# Started with SVN but allow bzrect bzr as well.  One needs to upgrade
# the shared repository ${PROJECT}.$SF/$PROJECT to --rich-root-pack first.
$BZR push bzr+ssh://${USER}@${PROJECT}.bzr.$SF/bzrroot/$PROJECT

# Push to SVN
$BZR push https://${PROJECT}.svn.$SF/svnroot/$PROJECT/trunk